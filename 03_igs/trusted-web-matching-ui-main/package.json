    "vue": "^3.3.4",
    "vue-chartjs": "^5.2.0",
    "vue-i18n": "^9.4.1",
    "vue-router": "^4.2.4",
    "vuetify": "^3.3.16",
    "vuex": "^4.1.0",
    "vuex-module-decorators": "^2.0.0"
  },
  "devDependencies": {
    "@mdi/font": "^7.2.96",
    "@storybook/vue3-vite": "^7.5.1",
    "@types/chart.js": "^2.9.38",
    "@types/node": "^20.6.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/ui": "^0.34.5",
    "@vue/test-utils": "^2.4.1",
    "react": "^18.2.0",
