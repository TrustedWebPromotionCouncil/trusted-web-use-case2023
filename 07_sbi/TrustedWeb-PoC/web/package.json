{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix-routes && remix build",
    "dev": "concurrently \"remix-routes -w\" \"remix dev\"",
    "start": "remix-serve build",
    "typecheck": "tsc --module nodenext",
    "format": "dprint fmt",
    "lint": "eslint --cache --cache-location ./.cache/eslint 'app/**/*.{ts,tsx}'",
    "test": "vitest",
    "testthru": "vitest run",
    "tsn": "ts-node -r tsconfig-paths/register -O '{\"module\": \"commonjs\"}'",
    "db:migrate": "yarn tsn app/db/cli.ts latest && kysely-codegen",
    "db:migrate:create": "yarn tsn app/db/cli.ts create",
    "db:migrate:rollback": "yarn tsn app/db/cli.ts rollback",
    "db:reset": "yarn tsn app/db/cli.ts reset",
    "db:seed": "yarn tsn app/db/cli.ts seed",
    "db:seed:create": "yarn tsn app/db/cli.ts createseed",
    "apitypegen": "openapi-zod-client ../corda-facade-openapi/openapi.yml -o app/features/corda/apiSchema.ts && openapi-zod-client ../openapi/openapi.yaml -o app/features/api/apiSchema.ts && yarn format"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11",
    "@emotion/server": "^11",
    "@emotion/styled": "^11",
    "@remix-run/css-bundle": "^1.18.1",
    "@remix-run/node": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@remix-run/serve": "^1.18.1",
    "@remix-validated-form/with-zod": "^2.0.6",
    "@zodios/core": "^10.9.4",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^6",
    "isbot": "^3.6.8",
    "kysely": "^0.26.1",
    "kysely-codegen": "^0.10.1",
    "lodash": "^4.17.21",
    "pg": "^8.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "remix-auth": "^3.5.0",
    "remix-auth-form": "^1.3.0",
    "remix-routes": "^1.4.4",
    "remix-validated-form": "^5.0.2",
    "tiny-invariant": "^1.3.1",
    "type-fest": "^4.2.0",
    "zod": "^3.21.4",
    "zod-form-data": "^2.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.18.1",
    "@remix-run/eslint-config": "^1.18.1",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "commander": "^11.0.0",
    "concurrently": "^8.2.0",
    "dprint": "^0.39.0",
    "eslint": "^8.38.0",
    "openapi-zod-client": "^1.10.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
